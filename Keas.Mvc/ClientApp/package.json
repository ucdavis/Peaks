{
  "name": "kaes-mvc",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "citest": "npx cross-env CI=true react-scripts test --coverage --coverageDirectory=./ --reporters default --reporters jest-trx-results-processor",
    "eject": "react-scripts eject",
    "testwatch": "jest --watchAll",
    "coverage": "jest --coverage --coverageReporters=cobertura",
    "lint": "node node_modules/eslint/bin/eslint.js ClientApp --ext .tsx,.ts,.js,.jsx"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@ucdavis/gunrockin": "^1.1.3",
    "bootstrap": "4.5.0",
    "date-fns": "2.14.0",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.5.1",
    "log4javascript": "1.4.15",
    "react": "^17.0.2",
    "react-bootstrap-typeahead": "5.1.0",
    "react-date-picker": "8.3.6",
    "react-dom": "^17.0.2",
    "react-error-overlay": "6.0.9",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "react-table": "^7.7.14",
    "react-toastify": "6.0.8",
    "reactstrap": "9.1.6",
    "stacktrace-js": "2.0.2",
    "yup": "0.29.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/date-fns": "2.6.0",
    "@types/history": "4.7.6",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.14",
    "@types/react": "^17.0.37",
    "@types/react-bootstrap-typeahead": "3.4.6",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-table": "^7.7.14",
    "@types/react-toastify": "4.1.0",
    "@types/reactstrap": "8.5.0",
    "@types/yup": "0.29.3",
    "eslint-config-prettier": "^8.3.0",
    "event-source-polyfill": "1.0.15",
    "jest-canvas-mock": "^2.3.1",
    "jest-trx-results-processor": "^2.2.0",
    "sass": "^1.62.1",
    "prettier": "2.0.5",
    "typescript": "^4.5.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "rules": {
      "no-restricted-globals": "warn"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/components/specs/styleMock.ts"
    }
  }
}
