{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 4],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "jsx-no-lambda": false
    },
    "rulesDirectory": []
}
